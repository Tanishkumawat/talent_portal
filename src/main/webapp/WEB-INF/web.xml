<?xml version="1.0" encoding="UTF-8"?>
<web-app version="5.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd">
    <servlet>
        <servlet-name>StudentDetailsWrite</servlet-name>
        <servlet-class>MyServelets.StudentDetailsWrite</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FacultyDetailsWrite</servlet-name>
        <servlet-class>MyServelets.FacultyDetailsWrite</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FacultySearch</servlet-name>
        <servlet-class>MyServelets.FacultySearch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getSkey</servlet-name>
        <servlet-class>MyServelets.getSkey</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>setSkey</servlet-name>
        <servlet-class>MyServelets.setSkey</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getFkey</servlet-name>
        <servlet-class>MyServelets.getFkey</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>setFkey</servlet-name>
        <servlet-class>MyServelets.setFkey</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentDetailsWrite</servlet-name>
        <url-pattern>/StudentDetailsWrite</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FacultyDetailsWrite</servlet-name>
        <url-pattern>/FacultyDetailsWrite</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FacultySearch</servlet-name>
        <url-pattern>/FacultySearch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getSkey</servlet-name>
        <url-pattern>/getSkey</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>setSkey</servlet-name>
        <url-pattern>/setSkey</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getFkey</servlet-name>
        <url-pattern>/getFkey</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>setFkey</servlet-name>
        <url-pattern>/setFkey</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
</web-app>
